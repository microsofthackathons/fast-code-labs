{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/images/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/api/*.json"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
